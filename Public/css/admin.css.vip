/*
	修改单选框样式（圆形）
*/
input[type="radio"].radio-circle {
    display: none;
}

input[type="radio"].radio-circle + i {
    display: inline-block;
    width: 1em;
    height: 1em;
    background-image: url('../images/radio.png');
    background-size: 100%;
    vertical-align: middle;
}

input[type="radio"].radio-circle:checked + i {
    background-image: url('../images/radio_checked.png');
}

input[type="radio"].radio-circle:disabled + i {
    background-image: url('../images/radio.png');
}

input[type="radio"].radio-circle:checked:disabled + i {
    background-image: url('../images/radio.png');
}
/*
	修改复选框样式（开关）
*/
.checkbox-switch {
    position: relative;
    display: inline-block;
    height: 20px;
    vertical-align: middle;
}

.checkbox-switch input[type="checkbox"] {
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    z-index: 5;
    opacity: 0;
    cursor: pointer;
    margin: 0;
    line-height: 20px;
}

.checkbox-switch label {
    position: relative;
    display: block;
    width: 50px;
    height: 20px;
    margin: 0;
    padding: 0;
    background: #a9a9a9;
    border-radius: 20px;
    -webkit-transition: 0.4s;
    transition: 0.4s;
}

.checkbox-switch label:before {
    position: absolute;
    top: 0;
    color: #fff;
    line-height: 20px;
    font-size: 12px;
    content: 'OFF';
    right: 8px;
    z-index: 1;
}

.checkbox-switch label:after {
    content: '';
    position: absolute;
    width: 16px;
    height: 16px;
    border-radius: 100%;
    left: 2px;
    top: 2px;
    z-index: 2;
    background: #fff;
    -webkit-transition: 0.4s;
    transition: 0.4s;
}

.checkbox-switch input:checked + label {
    background: #79b9fa;
}

.checkbox-switch input:checked + label:before {
    content: 'ON';
    right: auto;
    left: 11px;
}

.checkbox-switch input:checked + label:after {
    left: 30px;
}



/*
	修改下拉框样式
*/
