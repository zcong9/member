/*
	修改tab栏的样式
*/
.nav-tabs{
	padding-left: 15px;
	margin-bottom: 15px;
}
.nav-tabs>li{
	margin-right: 5px;
	margin-bottom: 0;
}
.nav-tabs>li>a,
.nav>li>a:focus, 
.nav>li>a:hover{
	padding: 5px 20px;
	border-radius: 0;
	background-color: #d5d5d5;
	color: #000;
	border-color: #b7b6b6;
	border-bottom-color: transparent;
}
.nav-tabs>li.active>a, 
.nav-tabs>li.active>a:focus, 
.nav-tabs>li.active>a:hover{
	border-color: #79b9fa;
	border-bottom-color: transparent;
	background-color: #79b9fa;
	color: #fff;
}
/*tab样式二（报表）*/
.nav-menu{
	list-style: none;
	margin-bottom: 0;
}
.nav-menu:after, 
.nav-menu:before{
	display: table;
	content: " ";
}
.nav-menu:after {
    clear: both;
}
.nav-menu>li{
	display: block;
	float: left;
	padding: 2px 12px;
	border: 1px solid #9c9c9c;
}
.nav-menu>li.active{
	background-color: #4485fd;
	border: 1px solid #4485fd;
}
.nav-menu>li.active a{
	color: #fff;
}
.nav-menu>li a{
